    <DatabaseInstall Type="post">
        <TableCreate Type="post" Name="ps_multi_smtp">
            <Column AutoIncrement="true" Name="id" PrimaryKey="true" Required="true" Type="INTEGER"></Column>
            <Column Name="type" Required="true" Size="250" Type="VARCHAR"></Column>
            <Column Name="host" Required="true" Size="250" Type="VARCHAR"></Column>
            <Column Name="smtp_user" Required="true" Size="200" Type="VARCHAR"></Column>
            <Column Name="smtp_password" Required="true" Size="250" Type="VARCHAR"></Column>
            <Column Name="authentication_type" Required="true" Size="250" Type="VARCHAR"></Column>
            <Column Name="oauth2_name" Required="false" Size="250" Type="VARCHAR"></Column>
            <Column Name="port" Required="true" Size="250" Type="VARCHAR"></Column>
            <Column Name="comments" Required="false" Size="250" Type="VARCHAR"></Column>
            <Column Name="encrypted" Required="true" Type="SMALLINT"></Column>
            <Column Name="create_time" Required="true" Type="DATE"></Column>
            <Column Name="create_by" Required="true" Type="INTEGER"></Column>
            <Column Name="valid_id" Required="true" Type="SMALLINT"></Column>
            <Column Name="change_time" Required="true" Type="DATE"></Column>
            <Column Name="change_by" Required="true" Type="INTEGER"></Column>
            <ForeignKey ForeignTable="users">
                <Reference Foreign="id" Local="create_by">
                </Reference>
                <Reference Foreign="id" Local="change_by">
                </Reference>
            </ForeignKey>
            <ForeignKey ForeignTable="valid">
                <Reference Foreign="id" Local="valid_id">
                </Reference>
            </ForeignKey>
        </TableCreate>
        <TableCreate Type="post" Name="ps_multi_smtp_address">
            <Column AutoIncrement="true" Name="id" PrimaryKey="true" Required="true" Type="INTEGER"></Column>
            <Column Name="smtp_id" Required="true" Type="INTEGER"></Column>
            <Column Name="address" Required="true" Size="250" Type="VARCHAR"></Column>
        </TableCreate>
    </DatabaseInstall>
    <DatabaseUpgrade Type="post">
        <TableAlter Type="post" Name="ps_multi_smtp" Version="3.1.1">
            <ColumnAdd Name="comments" Required="false" Size="250" Type="VARCHAR"></ColumnAdd>
        </TableAlter>
        <TableAlter Type="post" Name="ps_multi_smtp" Version="3.2.2">
            <ColumnChange NameNew="stmp_user" NameOld="user" Required="true" Size="200" Type="VARCHAR"></ColumnChange>
            <ColumnChange NameNew="stmp_password" NameOld="password" Required="true" Size="250" Type="VARCHAR"></ColumnChange>
        </TableAlter>
        <TableAlter Type="post" Name="ps_multi_smtp" Version="6.3.1">
            <ColumnAdd Name="authentication_type" Required="true" Size="250" Type="VARCHAR"></ColumnAdd>
            <ColumnAdd Name="oauth2_name" Required="false" Size="250" Type="VARCHAR"></ColumnAdd>
        </TableAlter>
    </DatabaseUpgrade>
    <DatabaseUninstall Type="pre">
        <TableDrop Type="pre" Name="ps_multi_smtp_address">
        </TableDrop>
        <TableDrop Type="pre" Name="ps_multi_smtp">
        </TableDrop>
    </DatabaseUninstall>
</otrs_package>